{
  "name": "kjk-engine-sandbox",
  "version": "0.1.0",
  "dependencies": [
    "sdl3",
    {
      "name": "sdl3-image",
      "features": [
        "jpeg",
        "png"
      ]
    },
    "sdl3-ttf",
    "entt",
    "box2d",
    "glm",
    "cpr",
    "enet",
    "onnxruntime",
    "spdlog",
    "glad",
    {
      "name": "imgui",
      "features": [ "docking-experimental", "sdl3-binding", "opengl3-binding" ]
    },
    {
      "name": "opengl",
      "platform": "windows"
    }
  ]
}